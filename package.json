{
  "name": "angular-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --no-watch --browsers ChromeHeadless",
    "test:coverage": "ng test --no-watch --code-coverage",
    "coverage:report": "open coverage/angular-app/index.html",
    "prettier": "prettier --write \"src/**/*.{ts,html,css,scss}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,html,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,scss}\"",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:a11y": "eslint --ext .ts,.html src/ --config eslint.config.js",
    "prepare": "husky && husky install",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --configuration=production --headless --browser=chrome --watch=false",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,html,css,scss}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "bootstrap": "^5.3.5",
    "bootstrap-icons": "^1.11.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.7",
    "@angular/cli": "^19.2.7",
    "@angular/compiler-cli": "^19.2.0",
    "@cypress/schematic": "^3.0.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "19.3.0",
    "axe-core": "^4.10.3",
    "cypress": "^14.3.0",
    "eslint": "^9.23.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^8.0.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.2.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.27.0"
  }
}
